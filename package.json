{
  "name": "we-plugin-auth",
  "description": "We.js authentication plugin, add suport to passport and auth routes",
  "version": "1.1.3",
  "main": "plugin.js",
  "scripts": {
    "test": "NODE_ENV=test LOG_LV=info ./node_modules/.bin/mocha test/bootstrap.js test/**/*.test.js",
    "coverage": "NODE_ENV=test LOG_LV=info node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- -u exports -R spec test/bootstrap.js test/**/*.test.js"
  },
  "dependencies": {
    "bcryptjs": "2.3.0",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "request": "^2.73.0"
  },
  "devDependencies": {
    "async": "^2.0.0",
    "express-mysql-session": "^1.1.1",
    "fs-extra": "^0.30.0",
    "istanbul": "^0.4.4",
    "mocha": "^2.5.3",
    "mysql": "^2.11.1",
    "rimraf": "^2.5.3",
    "sinon": "^1.17.4",
    "supertest": "^1.2.0",
    "we-core": "wejs/we-core",
    "we-plugin-acl": "wejs/we-plugin-acl",
    "we-plugin-email": "wejs/we-plugin-email",
    "we-plugin-form": "wejs/we-plugin-form",
    "we-plugin-user": "wejs/we-plugin-user",
    "we-test-tools": "wejs/we-test-tools"
  },
  "files": [
    "commands",
    "files/public",
    "lib",
    "locales",
    "server",
    "plugin.js",
    "install.js"
  ],
  "repository": "wejs/we-plugin-auth",
  "license": "MIT",
  "author": "Alberto Souza <contato@albertosouza.net>",
  "keywords": [
    "authentication",
    "wejs-plugin"
  ],
  "wejs": {
    "dependencies": {
      "we-plugin-acl": true,
      "we-plugin-user": true
    },
    "plugins": {
      "we-plugin-acl": true,
      "we-plugin-email": true,
      "we-plugin-form": true,
      "we-plugin-user": true
    },
    "devPlugins": {}
  }
}